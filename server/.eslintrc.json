{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "no-console": "off",
    "camelcase": "off",
    "prettier/prettier": "error"
  },
  "plugins": [
    "prettier"
  ]
}
