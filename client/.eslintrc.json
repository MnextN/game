{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier", "plugin:import/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "func-names": ["error", "never"],
        "no-console": "off",
        //"no-plusplus": 0,
        "prettier/prettier": "error"
    },
    "plugins": ["prettier"]
}
